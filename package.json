  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sontakey/ppg-js.git"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "fft.js": "^4.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.9.4",
    "rollup-plugin-postcss": "^4.0.2"
  }
}
