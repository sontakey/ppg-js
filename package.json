{
  "name": "ppg-js",
  "version": "1.0.0",
  "description": "Real-time PPG signal monitoring with camera-based photoplethysmography",
  "main": "dist/ppg-monitor.js",
  "module": "dist/ppg-monitor.esm.js",
  "browser": "dist/ppg-monitor.min.js",
  "style": "dist/ppg-monitor.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
